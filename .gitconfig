[branch]
  autosetuprebase = always
[alias]
  r = rebase
  co = checkout
  ci = commit
  st = status
  lol = log --graph --abbrev-commit --decorate --date-order --all --format=format:'%C(blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
